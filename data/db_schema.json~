{
    "jewelry": "CREATE TABLE jewelry (id INTEGER PRIMARY KEY NOT NULL, name VARCHAR(50) UNIQUE NOT NULL COLLATE NOCASE, photo_url VARCHAR(200) NOT NULL, price FLOAT NOT NULL, msmts TEXT);",

    "user": "CREATE TABLE user (id INTEGER PRIMARY KEY NOT NULL, fullname VARCHAR(100) NOT NULL, displayname varchar(50) NOT NULL, password VARCHAR(128) NOT NULL, salt VARCHAR(32) NOT NULL, email VARCHAR(320) UNIQUE NOT NULL, time_joined INTEGER NOT NULL);",

    "cart" : "CREATE TABLE cart (id INTEGER PRIMARY KEY NOT NULL, time_added INTEGER NOT NULL, FOREIGN KEY(user_id) REFERENCES user(id) NOT NULL, FOREIGN KEY(jewelry_id) REFERENCES jewelry(id) NOT NULL);",

    "transaction" : "CREATE TABLE transaction (id INTEGER PRIMARY KEY NOT NULL, time_transaction INTEGER NOT NULL, FOREIGN KEY(user_id) REFERENCES user(id) NOT NULL, FOREIGN KEY(jewelry_id) REFERENCES jewelry(id) NOT NULL);"
